{
    "name": "terrytsang/lara-catalog",
    "type": "library",
    "description": ":Product Catalog in Laravel",
    "keywords": [
        "Terrytsang",
        "LaraCatalog"
    ],
    "homepage": "https://github.com/terrytsang/lara-catalog",
    "license": "MIT",
    "authors": [
        {
            "name": ":Terry Tsang",
            "email": ":terrytsang811@gmail.com",
            "homepage": ":terrytsang.com",
            "role": "Creator"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Terrytsang\\LaraCatalog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Terrytsang\\LaraCatalog\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
